$(document).ready(function(){

	$(".super_a").hover(function(){

		$(this).addClass("hovers")
				.siblings();

			$(".gogo_1").animate({top:"-30px",},"slow");
								

		},function(){ 


			$(this).removeClass("hovers")
					.siblings();
			$(".gogo_1").animate({top:"0px"},"slow");
							
					
	});


});